<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="523.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.OutsourcedPartController">
   <children>
      <Label fx:id="screenLabel" layoutX="50.0" layoutY="45.0" text="Add Outsourced Part">
         <font>
            <Font name="Arial Bold Italic" size="18.0" />
         </font>
      </Label>
      <RadioButton fx:id="inhouseButton" layoutX="106.0" layoutY="104.0" mnemonicParsing="false" onAction="#swapOutsourcedButtonHandler" text="In House">
         <toggleGroup>
            <ToggleGroup fx:id="partChooser" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="outsourcedButton" layoutX="227.0" layoutY="104.0" mnemonicParsing="false" text="Outsourced" toggleGroup="$partChooser" />
      <Label layoutX="107.0" layoutY="158.0" text="ID" />
      <TextField fx:id="idField" layoutX="180.0" layoutY="153.0" />
      <Label layoutX="107.0" layoutY="294.0" text="Price/Cost" />
      <TextField fx:id="priceField" layoutX="180.0" layoutY="289.0" />
      <Label layoutX="107.0" layoutY="249.0" text="Inv" />
      <TextField fx:id="invField" layoutX="180.0" layoutY="244.0" />
      <Label layoutX="107.0" layoutY="204.0" text="Name" />
      <TextField fx:id="nameField" layoutX="180.0" layoutY="199.0" />
      <Label layoutX="76.0" layoutY="375.0" text="Company Name" />
      <TextField fx:id="companyField" layoutX="181.0" layoutY="370.0" />
      <Label layoutX="107.0" layoutY="336.0" text="Max" />
      <TextField fx:id="maxField" layoutX="180.0" layoutY="331.0" prefHeight="27.0" prefWidth="69.0" />
      <Label layoutX="276.0" layoutY="336.0" text="Min" />
      <TextField fx:id="minField" layoutX="349.0" layoutY="331.0" prefHeight="27.0" prefWidth="69.0" />
      <Button fx:id="saveButton" layoutX="380.0" layoutY="449.0" mnemonicParsing="false" onAction="#saveButtonHandler" text="Save" />
      <Button fx:id="cancelButton" layoutX="445.0" layoutY="449.0" mnemonicParsing="false" onAction="#cancelButtonHandler" text="Cancel" />
   </children>
</AnchorPane>
